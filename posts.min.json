{"pubDate":"{{ site.time | date_to_xmlschema }}","posts":[{% for post in site.posts %}{"title":"{{ post.title }}","url":"{{ post.url }}","category":"{{ post.categories }}","date":"{{ post.date | date_to_xmlschema }}","thumbnail":"{{ post.thumbnail | prepend: site.baseurl }}","coverimage":"{{ post.cover_image | prepend: site.baseurl }}","content":"{{ post.content | jsonify | replace: "img src", "img data-src" | replace: "data-original", "src" | cgi_escape }}"}{% if forloop.last %}{% else %},{% endif %}{% endfor %}]}
